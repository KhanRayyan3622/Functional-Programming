class assignment1 {
  object Rayyan {
    // Create an array of 10 integers, all initialized to 0
    val zeros = Array.fill(10)(0)

    // Create a two-dimensional array of 3 rows and 4 columns, all initialized to 1.0
    val ones = Array.fill(3, 4)(1.0)

    // Create an array of 5 random numbers between 0 and 1
    val randomNumbers = Array.fill(5)(math.random())

    // Create an array of 10 strings, each initialized to the same string
    val repeatedString = Array.fill(10)("Hello")

  }

}
